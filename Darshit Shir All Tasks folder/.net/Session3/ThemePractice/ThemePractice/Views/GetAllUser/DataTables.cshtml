@model IEnumerable<ThemePractice.Models.DataTable>

@{
    ViewBag.Title = "DataTableView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DataTableView</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
